{
	"Console Log":{
		"prefix": "cl",
		"body": "console.log($1)",
		"description": "Console Log"
	}, 
	
	"Console Log Working":{
		"prefix": "cw",
		"body": "console.log('working')",
		"description": "Console Log"
	}, 

	"Console Log Perfect":{
		"prefix": "cp",
		"body": "console.log('perfect')",
		"description": "Console Log"
	}, 

	"Name Function": {
		"prefix": "f",
		"body": [
		  "function ${1:functionName}($2){",
		  "",
		  "      $3",
		  "",
		  "}",
			"",
		  "${1:functionName}($4)"
		  
		],
		"description": "Name Function"
	  },

	  "Arrow Function": {
		"prefix": "af",
		"body": [
		  "const ${1:functionName} = ($2) =>{",
		  "",
		  "       $3",
		  "",
		  "}",
		  "",
		  "${1:functionName}($4)"	
		],
		"description": "Arrow Function"
	  },

	  "Arrow Function Only": {
		"prefix": "afu",
		"body": [
		  "const ${1: functionName} = ($2) =>{",
		  "",
		  "   $3",
		  "",
		  "}"
		],
		"description": "Arrow Function"
	  },

	  "Arrow Function Symbol":{
        "prefix": "afs",
		"body": ["=>{",
		  "",
		  "   $1",
		  "",
		  "}"	
		],
		"description": "Arrow Function Symbol"
	  },

	  "Array Method": {
		"prefix": "arme",
		"body": [
       "${1:|array}.${2|forEach, map, filter, reduce, some|}((${3:item}) =>{",
		  "",
		  "      $4",
		  "",
		  "})"
		],
		"description": "Array Method"
	  },

	  "Add Event Listener Linear":{
		"prefix": "ael",
		"body": "${1:element}.addEventListener(\"${2:click}\", ${3:functionName})",
		"description": "Add Event listener Linear"
	  },

	  "Query Selector": {
		  "prefix": "q",
		  "body": "document.querySelector('$1')",
		  "description": "Query Selector"
	  },

	  "Query Selector All": {
		  "prefix": "qa",
		  "body": "document.querySelectorAll('$1')",
		  "description": "Query Selector All"
	  },

	  "Class Lists":{
		"prefix": "cll",
		"body": "${1:element}.classList.${2|add,remove,toggle,contains|}(\"$3\")",
		"description": "Class Lists"	
	   },

		"Add Event Listener": {
			"prefix": "ae",
			"body": [
				"${1:element}.addEventListener(\"${1:click}\", () =>{",
				"",
				"    $3",
				"",		 
			"})"
			],
			"description": "Add Event listener"
		},

		"Create Element":{
			"prefix": "ce",
			"body": "const $1 = document.createElement(\"$1\")",
			"description": "Create Element"
		},

		"bar": {
			"prefix": "ba",
			"body": [
			 "let bar = document.querySelector('.bar')",
			"",
			"let menuToggler = document.querySelector('.menuToggler')",
			"",
			"console.log(bar, menuToggler)",
			"",
			"menuToggler.addEventListener('click', $1)",
			"",
			"",
			"function $1(){",
			"", 
				  "      console.log('working')",
			"",
				 "       bar.classList.toggle(\"$1\")",
			"",
            "}"
            			
			],
			"description": "bar"
		
		},

		"getAttribute": {
			"prefix": "ga",
			"body": "this.getAttribute('$1')",
			"description": "Get Attribute"
		}, 

		"setAttribute":{
			"prefix": "sat",
			"body": "setAttribute('${1:attribute}', '${2:value}')",
			"description": "Set Attribute"
		}
	
	}