{
	"Console Log":{
		"prefix": "cl",
		"body": "console.log($1)",
		"description": "Console Log"
	}, 
	
	"Console Log Working":{
		"prefix": "cw",
		"body": "console.log('working')",
		"description": "Console Log"
	}, 

	"Console Log Perfect":{
		"prefix": "cp",
		"body": "console.log('perfect')",
		"description": "Console Log"
	}, 

	"Name Function": {
		"prefix": "f",
		"body": [
		  "function ${1:functionName}(${2:${3:parameter}:${4:type}}){",
		  "",
		  "  $5",
		  "",
		  "}",
		  "",
		  "${1:functionName}($6)"
		],
		"description": "Name Function"
	},


	"Arrow Function":{
		"prefix": "af",
		"body": [
		  "const ${1:functionName} = (${2:${3:parameter}:${4:type}}) =>{",
		  "",
		  "  $5",
		  "",
		  "};",
		  "",
		  "${1:functionName}($6)"
		],
		"description": "Arrow Function"
	},
	  
	"Arrow Function Only":{
		"prefix": "afu",
		"body": [
		  "const ${1:functionName} = (${2:${3:parameter}:${4:type}}) =>{",
		  "",
		  "  $5",
		  "",
		  "}"
		],
		"description": "Arrow Function"
	},

	"Name Function Async":{
		"prefix": "fa",
		"body": [
		  "async function ${1:functionName}(${2:${3:parameter}:${4:type}}) {",
		  "",
		  "  $5",
		  "",
		  "}"
		],
		"description": "Name Function Async"
	},

	"Arrow Function Async":{
		"prefix": "afa",
		"body": [
		  "const ${1:functionName} = async (${2:${3:parameter}:${4:type}}) =>{",
		  "",
		  "  $5",
		  "",
		  "};"
		],
		"description": "Arrow Function Async"
	},

	"Arrow Function Symbol":{
		"prefix": "afs",
		"body": ["=>{",
			"",
			"   $1",
			"",
			"}"	
		],
		"description": "Arrow Function Symbol"
	},

	"Array Method":{
		"prefix": "arme",
		"body": [
		"${1:|array}.${2|forEach, map, filter, reduce, some|}((${3:item}) =>{",
			"",
			"      $4",
			"",
			"})"
		],
		"description": "Array Method"
	},

	"Add Event Listener Linear":{
		"prefix": "ael",
		"body": "${1:element}.addEventListener(\"${2:click}\", ${3:functionName})",
		"description": "Add Event listener Linear"
	},

	"Query Selector": {
		"prefix": "c",
		"body": "const $1 = document.querySelector(\"${2|.,#|}$1\") as $3",
		"description": "Query Selector"
	},

	"Query Selector All": {
		"prefix": "ca",
		"body": "const $1 = document.querySelectorAll(\"${2|.,#|}$1\") as $3",
		"description": "Query Selector All"
	},

	"Query Selectors":{
		"prefix": "q",
		"body": "$1 document.querySelector(\"${2|.,#|}$1\") as $3",
		"description": "Query Selectors"
	},

	"Query Selectors All":{
		"prefix": "qa",
		"body": "$1 document.querySelectorAll(\"${2|.,#|}$1\") as $3",
		"description": "Query Selectors All"
	},

	"Class Lists":{
		"prefix": "cll",
		"body": "${1:element}.classList.${2|add,remove,toggle,contains|}(\"$3\")",
		"description": "Class Lists"	
	},

	"Add Event Listener":{
		"prefix": "ae",
		"body": [
			"${1:element}.addEventListener(\"${1:click}\", () =>{",
			"",
			"    $3",
			"",		 
		"})"
		],
		"description": "Add Event listener"
	},

	"Create Element":{
		"prefix": "ce",
		"body": "const $1 = document.createElement(\"$1\")",
		"description": "Create Element"
	},

	"getAttribute":{
		"prefix": "ga",
		"body": "this.getAttribute('$1')",
		"description": "Get Attribute"
	}, 

	"setAttribute":{
		"prefix": "sat",
		"body": "setAttribute('${1:attribute}', '${2:value}')",
		"description": "Set Attribute"
	},

	"New Promise":{
		"prefix": "np",
		"body": [			
			"new Promise((resolve, reject) =>{", 
				"",
				"    $1",
				"",     
			"})"
		],
		"description": "New Promise"
	}

}