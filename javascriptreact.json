{
	"Stateless Arrow Function":{
		"prefix": "saf",
		"body": [
			"const ${1:FunctionName} = ($2) =>{",
				"",
				"    return(",
					"",
					"        $3",
					"",
				"    )",
			"",
			"}",			 
			"",
			"export default ${1:FunctionName}"
		],
        "description": "Stateless Arrow Function"
	},

    "Stateless function":{
        "prefix": "sf",
        "body":[
			"function ${1:FunctionName}($2){",
			"",
			"    return(",
				 "",
				 "         $3",
				 "",
				 ")",
			"",
			"}",
			"",
			"export default ${1:FunctionName}"
		],
		"description":"Stateless Function"
    },

	"Name Function Only": {
		"prefix": "f",
		"body": [
		  "function ${1:functionName}($2){",
		  "",
		  "   $3",
		  "",
		  "}"
		  
		],
		"description": "Name Function"
	},

	"Arrow Function Only": {
		"prefix": "af",
		"body": [
		  "const ${1: functionName} = ($2) =>{",
		  "",
		  "   $3",
		  "",
		  "}"
		],
		"description": "Arrow Function"
	},

	"Console Log":{
		"prefix": "cl",
		"body": "console.log($1)",
		"description": "Console Log"
	}, 

	"Console Log Working":{
		"prefix": "cw",
		"body": "console.log('working')",
		"description": "Console Log Working"
	},

	"Console Log Perfect":{
		"prefix": "cp",
		"body": "console.log('perfect')",
		"description": "Console Log Perfect"
	},

	"useState":{
		"prefix": "ust",
		"body": "const [${1:first}, set${2:$1}] = useState($3)",
		"description": "useState"
	}

}